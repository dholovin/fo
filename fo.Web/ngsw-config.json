{
  "appData": "[fo] any data that describes this particular version of the app",
  "index": "/index.html",
  "dataGroups": [
    {
      "name": "api-people",
      "urls": [
        "/api/people/*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "10s", // 'timeout' is considered for 'freshness' statagy
        "maxAge": "3d",   // 'maxAge' is considered for 'performance' strategy
        "maxSize": 100    // max number of entries or responses in the cache
      }
    }
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/manifest.json"
        ]
      }
    },
    {
      "name": "fonts",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/*.woff",
          "/*.woff2",
          "/*.ttf"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ]
}