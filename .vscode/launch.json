{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome with ng serve",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/fo.Web"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome with http-server",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/fo.Web"
        },
        {
            "name": "Launch Chrome with ng test",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:9876/debug.html",
            "webRoot": "${workspaceRoot}/fo.Web"
        },
        {
            "name": "Launch ng e2e",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/fo.Web/node_modules/protractor/bin/protractor",
            "protocol": "inspector",
            "args": [
                "${workspaceRoot}/fo.Web/e2e/protractor.conf.js"
            ]
        }
    ]
}